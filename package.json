{
  "name": "@zentla/sdk",
  "version": "0.2.0",
  "description": "Official TypeScript SDK for Zentla - subscription management API",
  "author": "hexrift",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hexrift/zentla-sdk.git"
  },
  "homepage": "https://zentla.dev/docs",
  "bugs": {
    "url": "https://github.com/hexrift/zentla-sdk/issues"
  },
  "keywords": [
    "zentla",
    "subscription",
    "billing",
    "saas",
    "api",
    "sdk",
    "typescript"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@4.6.0"
}
